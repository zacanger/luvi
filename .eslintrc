{
  "extends": [
    "standard"
  ],
  "plugins": [ "babel" ],
  "parser": "babel-eslint",
  "rules": {
    "comma-style": [1, "first"],
    "no-multi-spaces": [0],
    "object-curly-spacing": [1, "always"],
    "one-var": [0],
    "block-spacing": [0],
    "indent": [1],
    "arrow-parens": ["warn", "always"],
    "key-spacing": [0]
  }
}
